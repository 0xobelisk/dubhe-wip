{
  "name": "create-dubhe",
  "version": "0.1.15",
  "type": "module",
  "license": "MIT",
  "author": "team@obelisk.build",
  "bin": {
    "create-dubhe": "index.js"
  },
  "files": [
    "index.js",
    "dist",
    "templates"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "pnpm run build:js",
    "build:js": "tsup && pnpm run copy-templates && npm run cocos-js-build",
    "clean": "pnpm run clean:js",
    "clean:js": "shx rm -rf dist",
    "copy-templates": "tsx ./scripts/copy-templates.ts",
    "cocos-js-build": "cd cocos-lib-builder && npm i && browserify aptos.js -p esmify >  ../template/cocos/aptos-template/assets/lib/dubhe.js && browserify sui.js -p esmify >  ../template/cocos/sui-template/assets/lib/dubhe.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "validate": "pnpm format:check && pnpm type-check"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^18.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xobelisk/dubhe.git"
  },
  "dependencies": {
    "browser-resolve": "^2.0.0",
    "browser-sync": "^2.29.3",
    "fast-glob": "^3.3.3",
    "yargs-interactive": "^3.0.1"
  },
  "devDependencies": {
    "@types/yargs-interactive": "^2.1.6",
    "@types/cross-spawn": "^6.0.2",
    "@types/minimist": "^1.2.2",
    "@types/prompts": "^2.4.3",
    "cross-spawn": "^7.0.3",
    "kolorist": "^1.7.0",
    "prompts": "^2.4.2",
    "tsc": "^2.0.4",
    "unbuild": "^1.1.2",
    "esmify": "^2.1.1",
    "browserify": "^17.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3"
  }
}
